{
  "name": "generator-react-exhibit",
  "version": "4.0.0",
  "description": "Easily generate your own react component library, documentation included.",
  "homepage": "https://github.com/au-re/generator-react-exhibit",
  "author": {
    "name": "Aurelien Franky",
    "email": "au.re@live.com",
    "url": "http://au-re.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "react",
    "create-react-app",
    "styled-components",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^2.4.1",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.0",
    "cz-emoji": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "nsp": "^3.2.1",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "config": {
    "cz-emoji": {},
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "clean": "rm -rf node_modules && npm i",
    "commit": "git-cz",
    "prepublishOnly": "nsp check",
    "pretest": "./node_modules/.bin/eslint .",
    "precommit": "lint-staged",
    "test": "jest __tests__"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "au-re/generator-refresh",
  "license": "MIT"
}
