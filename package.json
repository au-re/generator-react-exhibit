{
  "name": "generator-react-exhibit",
  "version": "6.1.0",
  "description": "Easily generate your own react component library, documentation included.",
  "homepage": "https://github.com/au-re/generator-react-exhibit",
  "repository": "https://github.com/au-re/generator-react-exhibit",
  "license": "MIT",
  "author": {
    "name": "Aurelien Franky",
    "email": "au.re@live.com",
    "url": "http://au-re.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "react",
    "create-react-app",
    "styled-components",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "^4.0.0",
    "yeoman-generator": "^4.9.0",
    "yosay": "^2.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.4",
    "coveralls": "^3.0.11",
    "cz-emoji": "^1.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.5.3",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.4.1"
  },
  "config": {
    "cz-emoji": {},
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "clean": "rm -rf node_modules && npm i",
    "commit": "git-cz",
    "pretest": "./node_modules/.bin/eslint .",
    "precommit": "lint-staged",
    "test": "jest __tests__"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  }
}
